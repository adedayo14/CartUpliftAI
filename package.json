    "@vercel/remix": "2.16.7",
    "isbot": "^5.1.0",
    "lru-cache": "^11.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^5.0.1"
