{
  "name": "cart-uplift-app",
  "private": true,
  "scripts": {
    "build": "prisma generate && remix vite:build",
    "vercel-build": "prisma generate && prisma db push --accept-data-loss && remix vite:build",
    "postinstall": "prisma generate",
    "dev": "shopify app dev",
    "clean": "rm -rf node_modules build",
    "db:reset": "prisma migrate reset --force --skip-seed",
    "reset": "npm run clean && npm ci && prisma generate && prisma db push && npm run dev",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "remix-serve ./build/server/index.js",
    "docker-start": "npm run setup && npm run start",
    "setup": "prisma generate && prisma db push",
    "remix:build": "remix vite:build",
    "build:prod": "NODE_ENV=production prisma generate && remix vite:build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "shopify": "shopify",
    "prisma": "prisma",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite",
    "ml:seed": "node scripts/ml-seed-orders.js",
    "inv:increase": "node scripts/bulk-inventory-increase.js"
  },
